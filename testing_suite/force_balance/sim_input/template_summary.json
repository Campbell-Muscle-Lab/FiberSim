{
    "layout":
    {
        "fig_width": 7,
        "top_margin": 0,
        "left_margin": 0,
        "right_margin": 0,
        "panel_height": 2,
        "grid_wspace": -0.5
    },
    "x_display":{
        "global_x_field": "time",
        "label": "Time (s)"
    },
    "formatting":
    {
        "x_label_fontsize": 12,
        "y_label_fontsize": 12,
        "tick_fontsize": 12,
        "legend_fontsize": 12,
        "y_label_pad": 25,
        "legend_bbox_to_anchor": [1.0, 1],
        "patch_alpha": 0.3
    },
  "panels": [
    {
      "column": 1,
      "y_info": {
        "label": "pCa",
        "scaling_type": "close_fit",
        "series": [
          {
            "field": "hs_1_pCa"
          }
        ]
      }
    },
    {
      "column": 1,
      "y_info": {
        "label": "HS length\n(nm)",
        "ticks": [ 990, 1010 ],
        "series": [
          {
            "field": "hs_1_length"
          }
        ]
      }
    },
    {
      "column": 1,
      "y_info": {
        "label": "Stress\n(kN m$^{-2}$)",
        "series": [
          {
            "field": "hs_1_force",
            "field_label": "Total",
            "scaling_factor": 0.001
          },
          {
            "field": "hs_1_titin_force",
            "field_label": "Titin",
            "scaling_factor": 0.001
          }
        ]
      }
    },
    {
      "column": 1,
      "y_info": {
        "label": "Thin\nfilament",
        "series": [
          {
            "field": "hs_1_a_pop_1",
            "field_label": "N$_{off}$"
          },
          {
            "field": "hs_1_a_pop_2",
            "field_label": "N$_{on}$"
          }
        ]
      }
    },
    {
      "column": 1,
      "y_info": {
        "label": "Thick\nfilament",
        "series": [
          {
            "field": "hs_1_m_pop_1",
            "field_label": "M$_{1}$"
          },
          {
            "field": "hs_1_m_pop_2",
            "field_label": "M$_{2}$"
          },
          {
            "field": "hs_1_m_pop_3",
            "field_label": "M$_{3}$"
          }
        ]
      }
    },
    {
      "column": 1,
      "y_info": {
        "label": "MyBP-C",
        "scaling_type": "close_fit",
        "series": [
          {
            "field": "hs_1_c_pop_1",
            "field_label": "C$_{1}$"
          },
          {
            "field": "hs_1_c_pop_2",
            "field_label": "C$_{2}$"
          }
        ]
      }
    }
  ]
}